{
  "name": "react-native-basic-project",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "start:ip": "APP_ENV=ip react-native start",
    "start:dev": "APP_ENV=dev react-native start",
    "start:staging": "APP_ENV=staging react-native start",
    "start:prod": "APP_ENV=prod react-native start",
    "test:clearCache": "jest --clearCache",
    "test": "npm run test:clearCache && jest",
    "test:watch": "npm run test:clearCache && jest --watchAll",
    "test:coverage": "npm run test:clearCache && jest --collectCoverage --coverageDirectory=\"./coverage\"",
    "prepare": "husky"
  },
  "dependencies": {
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "@react-navigation/stack": "^6.3.29",
    "@reduxjs/toolkit": "^2.2.3",
    "async-mutex": "^0.5.0",
    "react": "18.2.0",
    "react-native": "0.74.0",
    "react-native-gesture-handler": "^2.16.1",
    "react-native-safe-area-context": "^4.10.1",
    "react-native-screens": "^3.31.1",
    "react-redux": "^9.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-transform-private-methods": "^7.24.1",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@react-native/babel-preset": "0.74.81",
    "@react-native/eslint-config": "0.74.81",
    "@react-native/metro-config": "0.74.81",
    "@react-native/typescript-config": "0.74.81",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "commitlint-plugin-function-rules": "^4.0.0",
    "eslint": "^8.19.0",
    "husky": "^9.0.11",
    "jest": "^29.6.3",
    "lint-staged": "^15.2.2",
    "prettier": "2.8.8",
    "react-native-dotenv": "^3.4.11",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint",
      "prettier --write"
    ]
  }
}
