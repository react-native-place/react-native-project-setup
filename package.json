{
  "name": "react-native-basic-project",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "start:ip": "APP_ENV=ip react-native start",
    "start:dev": "APP_ENV=dev react-native start",
    "start:staging": "APP_ENV=staging react-native start",
    "start:prod": "APP_ENV=prod react-native start",
    "test:clearCache": "jest --clearCache",
    "test": "npm run test:clearCache && jest --testPathIgnorePatterns=\\(/__build__/ /__prod__/\\)",
    "test:build": "npm run test:clearCache && jest --testPathPattern=__build__",
    "test:prod": "npm run test:clearCache && jest --testPathPattern=__prod__",
    "test:all": "npm run test:clearCache && jest",
    "test:watch": "npm run test:clearCache && jest --watchAll --testPathIgnorePatterns=\\(/__build__/ /__prod__/\\)",
    "test:coverage": "npm run test:clearCache && jest --collectCoverage --coverageDirectory=\"./coverage\"",
    "prepare": "husky",
    "wdio": "wdio run ./e2e/wdio.conf.ts"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.24.0",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "@react-navigation/stack": "^6.3.29",
    "@reduxjs/toolkit": "^2.2.3",
    "async-mutex": "^0.5.0",
    "i18next": "^23.12.2",
    "react": "18.2.0",
    "react-i18next": "^15.0.0",
    "react-native": "0.74.4",
    "react-native-gesture-handler": "^2.18.0",
    "react-native-safe-area-context": "^4.10.8",
    "react-native-screens": "^3.32.0",
    "react-redux": "^9.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-transform-private-methods": "^7.24.1",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@react-native/babel-preset": "0.74.86",
    "@react-native/eslint-config": "0.74.86",
    "@react-native/metro-config": "0.74.86",
    "@react-native/typescript-config": "0.74.86",
    "@testing-library/webdriverio": "^3.2.1",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "@wdio/appium-service": "^8.39.1",
    "@wdio/cli": "^8.39.1",
    "@wdio/local-runner": "^8.39.1",
    "@wdio/mocha-framework": "^8.39.0",
    "@wdio/spec-reporter": "^8.39.0",
    "@wdio/visual-service": "^5.2.0",
    "appium": "^2.11.2",
    "appium-uiautomator2-driver": "^3.7.4",
    "appium-wait-plugin": "^3.0.2",
    "appium-xcuitest-driver": "^7.24.4",
    "babel-jest": "^29.6.3",
    "chai": "^5.1.1",
    "commitlint-plugin-function-rules": "^4.0.0",
    "eslint": "^8.19.0",
    "husky": "^9.1.4",
    "jest": "^29.6.3",
    "lint-staged": "^15.2.2",
    "mocha": "^10.7.0",
    "prettier": "2.8.8",
    "react-native-dotenv": "^3.4.11",
    "react-test-renderer": "18.2.0",
    "ts-node": "^10.9.2",
    "typescript": "5.0.4",
    "wdio-wait-for": "^3.0.11",
    "webdriverio": "^8.39.1"
  },
  "engines": {
    "node": ">=18"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint",
      "prettier --write"
    ]
  }
}